#include<stdio.h>
struct stu
{
	char name[20];
	int ron;
	int m1,m2,m3;
	int sum;
	float per;
};
int main()
{
	struct stu s1;
	printf("enter details : name,roll no,m1,m2,m3\n");
	scanf("%s%d%d%d%d",&s1.name,&s1.ron,&s1.m1,&s1.m2,&s1.m3);
	s1.sum=s1.m1+s1.m2+s1.m3;
	s1.per=s1.sum/3;
	printf("\nstudent details :\n");
	printf("\nname : %s",s1.name);
	printf("\nroll no : %d",s1.ron);
	printf("\nmark2 : %d",s1.m2);
	printf("\nmark3 : %d",s1.m3);
	printf("sum=%d",&s1.sum);
	printf("\npercent =%f %",s1.per);
}
